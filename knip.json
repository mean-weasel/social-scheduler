{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": [
    "src/main.tsx",
    "src/app/**/page.tsx",
    "src/app/**/layout.tsx",
    "src/app/**/route.ts"
  ],
  "project": ["src/**/*.{ts,tsx}"],
  "ignore": [
    "src/vite-env.d.ts",
    "src/app/**/*"
  ],
  "ignoreDependencies": [
    "@octokit/rest",
    "@typescript-eslint/eslint-plugin",
    "@typescript-eslint/parser",
    "eslint-plugin-react-hooks",
    "eslint-plugin-react-refresh",
    "gh-pages",
    "tsx",
    "@supabase/ssr",
    "@supabase/supabase-js"
  ],
  "playwright": {
    "entry": ["e2e/**/*.spec.ts", "playwright.config.ts"]
  },
  "vitest": {
    "entry": ["src/test/**/*.ts", "vitest.config.ts"]
  }
}
